<p id="notice"><%= notice %></p>

<h1>Account Movements</h1>

<table>
  <thead>
    <tr>
      <th>Account</th>
      <th>Value</th>
      <th>Type</th>
      <th>Origin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @account_movements.each do |account_movement| %>
      <tr>
        <td><%= account_movement.account_id %></td>
        <td><%= account_movement.value %></td>
        <td><%= account_movement.type %></td>
        <td><%= account_movement.origin %></td>
        <td><%= link_to 'Show', account_movement %></td>
        <td><%= link_to 'Edit', edit_account_movement_path(account_movement) %></td>
        <td><%= link_to 'Destroy', account_movement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Account Movement', new_account_movement_path %>
